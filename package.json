{
  "name": "dax-catalog-mvp",
  "version": "1.0.0",
  "description": "DAX Catalog - AI-powered Power BI documentation system with 7-agent pipeline architecture",
  "main": "dist/index.js",
  "bin": {
    "dax-catalog": "dist/enhanced-cli.js"
  },
  "scripts": {
    "build": "tsc",
    "build:clean": "rm -rf dist && npm run build",
    "dev": "ts-node src/enhanced-cli.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:local": "node test-local-bikeshare.js",
    "test:agents": "npm run build && npm run test:local",
    "test:api": "npm run build && node test-api-key.js",
    "setup": "node setup-env.js",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:legacy": "ts-node src/test.ts",
    "lint": "eslint src/**/*.ts tests/**/*.ts --fix",
    "lint:check": "eslint src/**/*.ts tests/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\" \"*.{js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\" \"*.{js,json,md}\"",
    "typecheck": "tsc --noEmit",
    "cli": "ts-node src/enhanced-cli.ts",
    "legacy-cli": "ts-node src/cli.ts",
    "start": "node dist/enhanced-cli.js",
    "validate": "npm run typecheck && npm run lint:check && npm run test",
    "prepare": "npm run build"
    ,
    "ui:setup": "npm --prefix web-ui install",
    "ui:start": "npm --prefix web-ui start",
    "ui:dev": "npm run build && npm --prefix web-ui install && npm --prefix web-ui start"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.60.0",
    "commander": "^12.0.0",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.0.0",
    "glob": "^10.4.5",
    "p-limit": "^7.1.1"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "eslint-import-resolver-typescript": "^3.5.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-promise": "^6.1.0",
    "eslint-plugin-security": "^1.7.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-watch-typeahead": "^2.2.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "powerbi",
    "dax",
    "documentation",
    "claude",
    "anthropic",
    "agent-pipeline",
    "business-intelligence",
    "ai-powered",
    "domain-agnostic",
    "stakeholder-ready"
  ],
  "license": "MIT",
  "author": "DAX Catalog Team",
  "homepage": "https://github.com/your-org/dax-catalog-mvp#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/dax-catalog-mvp.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/dax-catalog-mvp/issues"
  },
  "directories": {
    "doc": "docs",
    "test": "tests"
  }
}
